{
  "name": "marko-lemon",
  "description": "Markojs Component Library",
  "version": "0.0.1",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "lasso-babel-transform": "^3.0.0",
    "lasso-stylus": "^2.0.0",
    "marko": "^4.18.10",
    "marko-starter": "^2.0.4",
    "material-design-icons": "^3.0.1",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.6.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "marko-prettyprint": "^1.5.2",
    "prettier": "^1.15.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BRubinson/Lemon-Marko-Components"
  },
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production marko-starter build",
    "format": "prettier src/**/*.{js,css,less} --write && marko-prettyprint src",
    "lint": "eslint src",
    "serve": "NODE_ENV=production marko-starter serve-static",
    "start": "marko-starter server",
    "test": "npm run lint"
  },
  "private": true,
  "static-repo":"git@github.com:BRubinson/Lemon-Marko-Components.git#demo-site"
}
